<script>
    window.addEventListener('load', function () {
      $('#introOverlay').fadeOut();
    })

    // lightbox (magnific)
    $('a.thumbnail').magnificPopup({
        type:'image',
        gallery: {
            enabled: true
        }
    });

    $(document).on('click', '.scroll-top', function() {
        $('body,html').animate({
            scrollTop: 0
        }, 800);
        return false;
    });

    $(document).ready(function() {
        if($(window).width() > 991) {
            $.parallaxify();
        } else {
            // Mobile AOS set
            $('[data-aos]').attr('data-aos', 'opacity-slide-up');
            AOS.init({
                duration: 800
            });
        }
    });

    $('.s3slider').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        infinite: false,
        arrows: false,
        dots: true,
        autoplay: true
    });

    $(document).on('click', '.scroll-link', function() {
        scroll_to_top();
        return false;
    });

    $(document).on('click', '.question', function() {
        var $this = $(this);
        var position = $this.attr('data-position');
        var answer = $('.answer[data-position="'+position+'"]');
        var btn = $('.open-question[data-position="'+position+'"]');
        $('.answer.open').slideUp( 300, function() {
            $('.answer.open').parent().find('.question').find('.open-question').html('+');
            $('.answer.open').removeClass('open');
        });
        if(!answer.hasClass('open')) {
            answer.slideDown( 300, function() {
                answer.addClass('open');
                btn.html('-');
            });
        }
    });
    AOS.init({
        duration: 800
    });

</script>
